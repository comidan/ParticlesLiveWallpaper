<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="processing.test.particleslivewallpaper">
    <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="23"/>
    <uses-feature android:name="android.software.live_wallpaper"/>
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="ParticlesLiveWallpaper">
        <service android:label="ParticlesLiveWallpaper" android:name=".MainService" android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
            </intent-filter>
            <meta-data android:name="android.service.wallpaper" android:resource="@xml/wallpaper"/>
        </service>
        <activity android:name=".MainService$PermissionRequestActivity"/>
    </application>
</manifest>
